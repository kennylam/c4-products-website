  "license": "Apache-2.0",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 gatsby build --prefix-paths",
    "copy": "npx microsite-common copy",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development run-s copy 'gatsby develop'",
    "dev:clean": "run-s 'gatsby clean' dev",
    "gatsby": "gatsby",
    "reinstall": "echo Clearing npm cache; npm cache clean --force; echo Removing npm package-lock.json; rimraf package-lock.json; echo Removing package folders; rimraf node_modules; npm install",
    "serve": "cross-env NODE_OPTIONS=--max_old_space_size=4096 npm run gatsby serve"
  },
  "dependencies": {
    "@mdx-js/runtime": "^1.6.5",
    "@uiw/react-md-editor": "^1.14.3",
    "ace-builds": "^1.4.11",
    "buffer": "^6.0.3",
    "cdai-microsite-common-module": "git+ssh://git@github.ibm.com:cloud-integration-design/cdai-microsite-common.git#npm-next",
    "classnames": "^2.2.6",
    "framer-motion": "^2.5.1",
    "front-matter": "^3.2.1",
    "gatsby-cli": "^3.0.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-theme-carbon": "^2.0.0",
    "gatsby-transformer-json": "^3.0.0",
    "gatsby": "^3.0.0",
    "lodash": "^4.17.13",
    "node-sass": "6.0.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react-csv": "^2.2.2",
    "react-dom": "^16.13.1",
    "react-lottie": "^1.2.3",
    "react": "^16.13.1",
    "sass": "^1.43.4"
  },
